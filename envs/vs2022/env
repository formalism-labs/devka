_VS2022_LIB=(
	$VS_ROOT/VC/Tools/MSVC/14.44.35207/lib/x64
	$WINSDK_ROOT/lib/10.0.19041.0/ucrt/x64
	$WINSDK_ROOT/lib/10.0.19041.0/um/x64)
export VS2022_LIB=$(join_dirs _VS2022_LIB)

_VS2022_LIBPATH=(
	$VS_ROOT/VC/Tools/MSVC/14.44.35207/lib/x64
	$VS_ROOT/VC/Tools/MSVC/14.44.35207/lib/x86/store/references
